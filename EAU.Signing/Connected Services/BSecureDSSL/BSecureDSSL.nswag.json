          {
            "name": "xpathNamespaces",
            "in": "formData",
            "description": "XPath signature namespaces (XAdES Enveloped) delimited with comma.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "xpathPrefixes",
            "in": "formData",
            "description": "XPath signature prefixes (XAdES Enveloped) delimited with comma. ",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Data to sign retrieved successfully",
            "schema": {
              "$ref": "#/definitions/DigestResponseDto"
            }
          },
          "400": {
            "description": "Bad request, invalid criteria parameter.",
            "schema": {
              "$ref": "#/definitions/BadRequestErrorResponseDto"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/InternalServerErrorResponseDto"
            }
          }
        },
        "deprecated": false
      }
    },
