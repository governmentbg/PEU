          "b-secure-dssl-controller"
        ],
        "summary": "Validate signed document",
        "description": "Validate signed document producing simple and detailed report.",
        "operationId": "validateSignedDocumentUsingPOST",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "type": "string",
